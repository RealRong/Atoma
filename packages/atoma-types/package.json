{
    "name": "atoma-types",
    "version": "0.0.0",
    "type": "module",
    "description": "Atoma shared type exports",
    "exports": {
        "./shared": {
            "types": "./dist/shared/index.d.ts",
            "import": "./dist/shared.js",
            "source": "./src/shared/index.ts"
        },
        "./core": {
            "types": "./dist/core/index.d.ts",
            "import": "./dist/core.js",
            "source": "./src/core/index.ts"
        },
        "./runtime": {
            "types": "./dist/runtime/index.d.ts",
            "import": "./dist/runtime.js",
            "source": "./src/runtime/index.ts"
        },
        "./client": {
            "types": "./dist/client/index.d.ts",
            "import": "./dist/client.js",
            "source": "./src/client/index.ts"
        },
        "./client/client": {
            "types": "./dist/client/client.d.ts",
            "import": "./dist/client/client.js",
            "source": "./src/client/client.ts"
        },
        "./client/options": {
            "types": "./dist/client/options.d.ts",
            "import": "./dist/client/options.js",
            "source": "./src/client/options.ts"
        },
        "./client/plugins": {
            "types": "./dist/client/plugins/index.d.ts",
            "import": "./dist/client/plugins.js",
            "source": "./src/client/plugins/index.ts"
        },
        "./client/registry": {
            "types": "./dist/client/registry.d.ts",
            "import": "./dist/client/registry.js",
            "source": "./src/client/registry.ts"
        },
        "./client/ops": {
            "types": "./dist/client/ops.d.ts",
            "import": "./dist/client/ops.js",
            "source": "./src/client/ops.ts"
        },
        "./client/relations": {
            "types": "./dist/client/relations.d.ts",
            "import": "./dist/client/relations.js",
            "source": "./src/client/relations.ts"
        },
        "./client/schema": {
            "types": "./dist/client/schema.d.ts",
            "import": "./dist/client/schema.js",
            "source": "./src/client/schema.ts"
        },
        "./protocol": {
            "types": "./dist/protocol/index.d.ts",
            "import": "./dist/protocol.js",
            "source": "./src/protocol/index.ts"
        },
        "./protocol-tools": {
            "types": "./dist/protocol-tools/index.d.ts",
            "import": "./dist/protocol-tools.js",
            "source": "./src/protocol-tools/index.ts"
        },
        "./observability": {
            "types": "./dist/observability/index.d.ts",
            "import": "./dist/observability.js",
            "source": "./src/observability/index.ts"
        },
        "./sync": {
            "types": "./dist/sync/index.d.ts",
            "import": "./dist/sync.js",
            "source": "./src/sync/index.ts"
        },
        "./devtools": {
            "types": "./dist/devtools/index.d.ts",
            "import": "./dist/devtools.js",
            "source": "./src/devtools/index.ts"
        },
        "./internal": {
            "types": "./dist/internal/index.d.ts",
            "import": "./dist/internal.js",
            "source": "./src/internal/index.ts"
        }
    },
    "scripts": {
        "build": "tsup && tsc -p tsconfig.build.json",
        "typecheck": "tsc -p tsconfig.json --noEmit"
    },
    "dependencies": {
        "atoma-shared": "workspace:*",
        "immer": "^10.1.1"
    }
}
