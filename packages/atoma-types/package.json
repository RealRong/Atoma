{
    "name": "atoma-types",
    "version": "0.0.0",
    "type": "module",
    "description": "Atoma shared type exports",
    "exports": {
        "./shared": {
            "types": "./dist/shared/index.d.ts",
            "import": "./dist/shared.js",
            "source": "./src/shared/index.ts"
        },
        "./core": {
            "types": "./dist/core/index.d.ts",
            "import": "./dist/core.js",
            "source": "./src/core/index.ts"
        },
        "./runtime": {
            "types": "./dist/runtime/index.d.ts",
            "import": "./dist/runtime.js",
            "source": "./src/runtime/index.ts"
        },
        "./client": {
            "types": "./dist/client/index.d.ts",
            "import": "./dist/client.js",
            "source": "./src/client/index.ts"
        },
        "./protocol": {
            "types": "./dist/protocol/index.d.ts",
            "import": "./dist/protocol.js",
            "source": "./src/protocol/index.ts"
        },
        "./protocol-tools": {
            "types": "./dist/protocol-tools/index.d.ts",
            "import": "./dist/protocol-tools.js",
            "source": "./src/protocol-tools/index.ts"
        },
        "./observability": {
            "types": "./dist/observability/index.d.ts",
            "import": "./dist/observability.js",
            "source": "./src/observability/index.ts"
        },
        "./sync": {
            "types": "./dist/sync/index.d.ts",
            "import": "./dist/sync.js",
            "source": "./src/sync/index.ts"
        },
        "./devtools": {
            "types": "./dist/devtools/index.d.ts",
            "import": "./dist/devtools.js",
            "source": "./src/devtools/index.ts"
        },
        "./internal": {
            "types": "./dist/internal/index.d.ts",
            "import": "./dist/internal.js",
            "source": "./src/internal/index.ts"
        }
    },
    "scripts": {
        "build": "tsup && tsc -p tsconfig.build.json",
        "typecheck": "tsc -p tsconfig.json --noEmit"
    },
    "dependencies": {
        "atoma-shared": "workspace:*",
        "immer": "^10.1.1"
    }
}
