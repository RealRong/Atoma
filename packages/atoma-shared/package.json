{
    "name": "atoma-shared",
    "version": "0.0.0",
    "type": "module",
    "description": "Atoma shared utilities",
    "main": "dist/index.js",
    "module": "dist/index.js",
    "types": "dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js",
            "source": "./src/index.ts"
        }
    },
    "imports": {
        "#shared": "./src/index.ts",
        "#shared/*": "./src/*"
    },
    "scripts": {
        "build": "tsup",
        "typecheck": "tsc -p tsconfig.json --noEmit"
    },
    "dependencies": {
        "zod": "^4.2.1"
    }
}
