        "#sync/*": "./src/*"
    },
    "scripts": {
        "build": "tsup",
        "typecheck": "tsc -p tsconfig.json --noEmit"
    },
    "dependencies": {
        "atoma": "workspace:*",
        "idb": "^8.0.3"
    }
}
