{
    "name": "atoma-sync",
    "version": "0.0.0",
    "type": "module",
    "description": "Atoma Sync Engine (optional extension package)",
    "main": "dist/index.js",
    "module": "dist/index.js",
    "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
