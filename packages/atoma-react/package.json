{
    "name": "atoma-react",
    "version": "0.0.0",
    "type": "module",
    "description": "Atoma React Hooks (optional extension package)",
    "main": "dist/index.js",
    "module": "dist/index.js",
    "types": "dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js"
        }
    },
    "peerDependencies": {
        "atoma": "^1.0.0",
        "react": "^18.0.0 || ^19.0.0"
    },
    "devDependencies": {
        "atoma": "workspace:*"
    },
    "dependencies": {
        "atoma-shared": "workspace:*",
        "atoma-types": "workspace:*"
    },
    "scripts": {
        "build": "tsup",
        "typecheck": "tsc -p tsconfig.json --noEmit"
    }
}
