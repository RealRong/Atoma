{
    "name": "atoma-core",
    "version": "0.0.0",
    "type": "module",
    "description": "Atoma core store engine",
    "exports": {
        "./store": {
            "types": "./dist/store.d.ts",
            "import": "./dist/store.js",
            "source": "./src/store/index.ts"
        },
        "./query": {
            "types": "./dist/query.d.ts",
            "import": "./dist/query.js",
            "source": "./src/query/index.ts"
        },
        "./relations": {
            "types": "./dist/relations.d.ts",
            "import": "./dist/relations.js",
            "source": "./src/relations/index.ts"
        },
        "./indexes": {
            "types": "./dist/indexes.d.ts",
            "import": "./dist/indexes.js",
            "source": "./src/indexes/index.ts"
        },
        "./operation": {
            "types": "./dist/operation.d.ts",
            "import": "./dist/operation.js",
            "source": "./src/operation.ts"
        }
    },
    "scripts": {
        "build": "tsup",
        "typecheck": "tsc -p tsconfig.json --noEmit"
    },
    "dependencies": {
        "atoma-shared": "workspace:*",
        "atoma-protocol": "workspace:*",
        "atoma-types": "workspace:*"
    },
    "peerDependencies": {
        "immer": "^9.0.0 || ^10.0.0"
    }
}
